{
 
  "scripts": {
    "build": "dotnet build --configuration Release",
    "test": "dotnet test --configuration Release",
    "ci": "dotnet r build && dotnet r test",
    "packCommon": "dotnet pack RSCG_IFormattableCommon/RSCG_IFormattableCommon.csproj -o PACK --no-build -c Release ",
